{
  "name": "express-prisma-mysql",
  "version": "1.0.0",
  "description": "server with prisma and mysql db ",
  "main": "index.js",
  "repository": "https://github.com/CesarJOtizM/express-prisma-mysql",
  "author": "Cesar Ortiz (cesarjavierortizmontero@gmail.com)",
  "license": "MIT",
  "scripts": {
    "start": "ts-node ./src/server.ts",
    "dev": "DEBUG=app:*NODE_ENV=environment nodemon",
    "start:prod": "npm run build && NODE_ENV=production node ./dist/src/server.js",
    "build": "npx tsc",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma",
    "seed": "ts-node ./src/prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^3.3.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "easy-template-x": "^2.1.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.12",
    "prisma": "^3.1.1",
    "ts-node": "^10.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "typescript": "^4.4.2"
  }
}
